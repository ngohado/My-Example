<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.hado.myexample.rxjava.RXJavaActivity">

    <Button
        android:id="@+id/btn_observable_sync"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Observable Sync" />

    <Button
        android:id="@+id/btn_observable_async"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Observable Async" />

    <Button
        android:id="@+id/btn_subject"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Subject" />

    <Button
        android:id="@+id/btn_map"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Map" />

    <ImageView
        android:id="@+id/image"
        android:layout_width="50dp"
        android:layout_height="50dp" />

</LinearLayout>
